<Application x:Class="SubripFixer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SubripFixer"
             xmlns:converter="clr-namespace:SubripFixer.Converter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:TitleConverter x:Key="TitleConverter" />
        <converter:InverseBooleanConverter x:Key="InverseBooleanConverter" />
        <converter:EnvironmentConverter x:Key="EnvironmentConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    </Application.Resources>
</Application>
